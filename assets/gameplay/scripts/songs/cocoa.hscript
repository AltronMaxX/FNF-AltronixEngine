var triggeredAlready = false;

function onUpdate(elapsed)
{
	if (gf.getCurAnimName() == 'danceLeft'
		|| gf.getCurAnimName() == 'danceRight'
		|| gf.getCurAnimName() == 'idle')
	{
		if (curBeat < 170)
		{
			if (curBeat < 65 || curBeat > 130 && curBeat < 145)
			{
				if (curBeat % 16 == 15)
				{
					if (!triggeredAlready)
					{
						gf.playAnim('cheer');
						triggeredAlready = true;
					}
				}
				else
					triggeredAlready = false;
			}
		}
	}
}